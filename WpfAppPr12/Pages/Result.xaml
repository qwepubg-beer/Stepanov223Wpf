<Page x:Class="WpfAppPr12.Pages.Result"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfAppPr12.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="420" d:DesignWidth="800"
      Title="Result">

    <Grid>
        <StackPanel Margin="370,0,20,0">
            <Label Content="Введите фамилию"/>
            <TextBox TextWrapping="Wrap" Text="" Width="393"/>
            <Label Content="Введите имя "/>
            <TextBox TextWrapping="Wrap" Text="" Width="393"/>
            <Label Content="Введите отчество "/>
            <TextBox TextWrapping="Wrap" Text="" Width="393"/>
            <Label Content="Введите почту "/>
            <TextBox TextWrapping="Wrap" Text="" Width="393"/>
            <Label Content="Ваш способ оплаты"/>
            <ComboBox x:Name="PayType" Width="375"/>
            <Button Content="Отправить заявку"/>

        </StackPanel>
        <Image HorizontalAlignment="Left" Height="367" VerticalAlignment="Top" Width="350" Margin="0,30,0,0">
            <Image.Source>
                <FormatConvertedBitmap Source="/Перминов.jpg"
DestinationFormat="Gray32Float" />
            </Image.Source>
        </Image>
    </Grid>
</Page>
